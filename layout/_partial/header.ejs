<div class='profile'>
  <h1><a href="<%- config.root %>"><img class="logo" src="/css/img/logo.gif"/></a></h1>
  <h2>
  	<a href="<%- config.root %>">
  	<%= config.title %>
  	<% if (config.subtitle){ %><%= config.subtitle %><% } %>
  	</a>
  </h2>
</div>
<nav id="main-nav">
  <ul class='inner' style="text-align:center;">
    <% for (var i in theme.menu){ %>
      <li><a href="<%- theme.menu[i] %>"><%= i %></a></li>
    <% } %>
  </ul>
  <div class="clearfix"></div>
</nav>
<div class="clearfix"></div>